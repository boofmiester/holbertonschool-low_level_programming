#include "main.h"
#include <stdio.h>
/**
 * _memset - fills memory with a constant byte.
 * @s: pointer to the memory area
 * @b: the constant byte
 * @n: byte to fill in
 * Return: pointer to the memory area.
 */
char *_memset(char *s, char b, unsigned int n)
{
unsigned int i = 0;
char start = s;
while (i < n)
{
*s++ = b;
i++;
}
return (s);
}
